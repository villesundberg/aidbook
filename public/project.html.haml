          <tr>
          <td>
          <label>User ID:</label> <input name="UserID" type="text"  id="UserID" size="15"/>
          </td>
          <td>
          <label>Password:</label> <input name="Password" type="password" class="control" id="Password1" size="15"/>
          </td>
          </tr>
          </table>
        /
          <form id="quick-search" method="get" action="index.html">
          <fieldset class="search">
          <label for="qsearch">Search:</label>
          <input class="tbox" id="qsearch" type="text" name="qsearch" value="Search..." title="Start typing and hit ENTER" />
          <button class="btn" title="Submit Search">Search</button>
          </fieldset>
          </form>
