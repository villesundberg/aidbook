        %a{:name => "top"}
        %a#logo-link{:href => "index.html"}/
        %h1 Aidbook
        #slogan
          %span Sharing the good we do
        /
          <div  id="nav">
          <ul>
          <li id="current"><a href="index.html">Home</a></li>
          <li><a href="style.html">About</a></li>
          <li><a href="blog.html">Support</a></li>
